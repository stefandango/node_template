{
  "name": "node_template",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "module": "main.js",
  "scripts": {
    "test": "mocha -r esm **/*.test.js",
    "clean": "rimraf build",
    "lint": "eslint '**/*.js' --ignore-pattern node_modules/",
    "build:dev": "npm run lint && npm run clean && webpack --config webpack.dev.js",
    "build:prod": "npm run lint && npm run clean && webpack --config webpack.prod.js",
    "watch:client": " npm run clean && webpack --config webpack.dev.js --watch --progress",
    "watch:server": "nodemon --inspect-brk=0.0.0.0:9229 index.js --ignore 'build/' --exec 'npm test'",
    "start": "npm-run-all -p watch:client watch:server"
  },
  "keywords": [
    "Homepage",
    "fun",
    "playground"
  ],
  "author": "Stefan P",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "helmet": "^3.21.1",
    "jquery": "^3.4.1",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "autoprefixer": "^9.6.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-plugin-mocha": "^6.2.0",
    "exports-loader": "^0.7.0",
    "extract-loader": "^3.1.0",
    "favicons-webpack-plugin": "^1.0.2",
    "file-loader": "^4.2.0",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^0.5.5",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "supertest": "^4.0.2",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "webpack-mode": "^1.0.2"
  }
}
